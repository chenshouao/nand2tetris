/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
	Or(a = in[0], b = in[1], out = one0);
	Or(a = in[2], b = in[3], out = one1);
	Or(a = in[4], b = in[5], out = one2);
	Or(a = in[6], b = in[7], out = one3);
	Or(a = one0, b = one1, out = one4);
	Or(a = one2, b = one3, out = one5);
	
}